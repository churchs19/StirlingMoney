                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Path x:Name="PART_ArrowElement" Canvas.ZIndex="100" Fill="{TemplateBinding Background}" StrokeThickness="0" Visibility="Collapsed"/>
                        <Path x:Name="PART_PointerOverPathElement" Fill="{TemplateBinding Background}" StrokeThickness="0"  Canvas.ZIndex="100" Visibility="Collapsed"/>
